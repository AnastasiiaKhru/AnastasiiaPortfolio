    <script>
        document.querySelectorAll('nav a').forEach(a => a.addEventListener('click', () => document.querySelector('nav ul')?.classList.remove('show')));
        window.addEventListener('scroll', function() {
            var btn = document.getElementById('backToTop');
            btn?.classList.toggle('show', window.scrollY > 400);
            document.querySelector('header')?.classList.toggle('scrolled', window.scrollY > 50);
        });
        document.getElementById('backToTop')?.addEventListener('click', () => window.scrollTo({ top: 0, behavior: 'smooth' }));
        var observer = new IntersectionObserver(function(entries) { entries.forEach(function(e) { if (e.isIntersecting) e.target.classList.add('visible'); }); }, { threshold: 0.1, rootMargin: '0px 0px -50px 0px' });
        document.querySelectorAll('.reveal').forEach(function(el) { observer.observe(el); });
        function updateNavActive() {
            var sections = ['features','pricing','testimonials','faq','contact'];
            var scrollY = window.scrollY + 150;
            document.querySelectorAll('nav a[href^="#"]').forEach(function(a){ a.classList.remove('active'); });
            sections.forEach(function(id) {
                var el = document.getElementById(id);
                var link = document.querySelector('nav a[href="#' + id + '"]');
                if (el && link) {
                    var top = el.offsetTop, h = el.offsetHeight;
                    if (scrollY >= top && scrollY < top + h) link.classList.add('active');
                }
            });
        }
        window.addEventListener('scroll', updateNavActive);
        updateNavActive();

        (function() {
            var track = document.getElementById('testimonialsTrack');
            var dots = document.getElementById('testimonialsDots');
            if (!track) return;
            var slides = track.querySelectorAll('.testimonial');
            var total = slides.length;
            var idx = 0;
            function update() {
                var slideW = slides[0] ? slides[0].offsetWidth + 24 : 320;
                var maxIdx = Math.max(0, total - 1);
                idx = Math.max(0, Math.min(idx, maxIdx));
                track.style.transform = 'translateX(' + (-idx * slideW) + 'px)';
                dots.querySelectorAll('.carousel-dot').forEach(function(d, j){ d.classList.toggle('active', j === idx); });
            }
            function goTo(i) { idx = i; update(); }
            for (var i = 0; i < total; i++) {
                var dot = document.createElement('button');
                dot.type = 'button';
                dot.className = 'carousel-dot' + (i === 0 ? ' active' : '');
                dot.setAttribute('aria-label', 'Slide ' + (i + 1));
                dot.addEventListener('click', (function(k){ return function(){ goTo(k); }; })(i));
                dots.appendChild(dot);
            }
            document.querySelector('.carousel-prev')?.addEventListener('click', function(){ goTo(idx - 1); });
            document.querySelector('.carousel-next')?.addEventListener('click', function(){ goTo(idx + 1); });
            window.addEventListener('resize', update);
        })();

        (function() {
            var slides = document.querySelectorAll('.hero-slide');
            var dotsEl = document.getElementById('heroDots');
            if (slides.length && dotsEl) {
                var idx = 0;
                for (var i = 0; i < slides.length; i++) {
                    var d = document.createElement('button');
                    d.type = 'button';
                    d.className = 'hero-dot' + (i === 0 ? ' active' : '');
                    d.setAttribute('aria-label', 'Slide ' + (i + 1));
                    d.addEventListener('click', (function(k){ return function(){ goTo(k); }; })(i));
                    dotsEl.appendChild(d);
                }
                function goTo(i) {
                    idx = i;
                    slides.forEach(function(s, j){ s.classList.toggle('active', j === idx); });
                    dotsEl.querySelectorAll('.hero-dot').forEach(function(d, j){ d.classList.toggle('active', j === idx); });
                }
                setInterval(function(){ goTo((idx + 1) % slides.length); }, 5000);
            }
        })();
    </script>
